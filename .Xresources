Xft.dpi: 96
Xft.lcdfilter: lcddefault
Xft.antialias: true
Xft.autohint: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xcursor.theme: neutral

!-------------------------------------------------------------------------------
!
!
!      U R X V T
!
!
URxvt*font:             -*-terminus-medium-r-*-*-14-140-*-*-*-*-*-*
URxvt*boldFont:         -*-terminus-medium-r-*-*-14-140-*-*-*-*-*-*
URxvt*italicFont:       -*-terminus-medium-r-*-*-14-140-*-*-*-*-*-*
URxvt*boldItalicFont:   -*-terminus-medium-r-*-*-14-140-*-*-*-*-*-*

! URxvt*font:             xft:DejaVu Sans Mono:size=10
! URxvt*boldFont:         xft:DejaVu Sans Mono:bold:size=10

! URxvt*font:              xft:Ubuntu Mono:size=10
! URxvt*boldFont:          xft:Ubuntu Mono:bold:size=10

URxvt*saveLines:        32767
URxvt*scrollBar:        false
URxvt*scrollBar_right:  true
URxvt*loginShell:       true
URxvt*shading:          30
URxvt*termName:         rxvt-unicode
URxvt*secondaryScroll:  true
URxvt*perl-lib:         /usr/lib64/urxvt/perl/
URxvt*perl-ext-common:  default,matcher,searchable-scrollback
URxvt*url-launcher:     xdg-open
URxvt*matcher.button:   1
URxvt*visualBell:       true
URxvt*keysym.C-u:       perl:matcher
URxvt*urgentOnBell:     true

!-------------------------------------------------------------------------------
!
!
!      X T E R M
!
!
XTerm*internalBorder:        0
XTerm*loginShell:         true
XTerm*scrollBar:         false
XTerm*cursorBlink:       false
!XTerm*geometry:         80x26
XTerm*saveLines:         65535
XTerm*dynamicColors:        on
XTerm*highlightSelection: true
! Catch URLs?
XTerm*charClass:     33:48,35-38:48,39:43,42-47:48,58-59:48,61:48,63-64:48,126:48
! Appearance
XTerm*utf8:          2
XTerm*eightBitInput: true
XTerm*faceName:      terminus:pixelsize=12

URxvt*foreground:   #e6d4c3
URxvt*background:   #282222
URxvt*cursorColor:  #e6d4c3
URxvt*color0:       #222222
URxvt*color8:       #331c1c
URxvt*color1:       #997373
URxvt*color9:       #995454
URxvt*color2:       #999973
URxvt*color10:      #999954
URxvt*color3:       #997e63
URxvt*color11:      #cc9e70
URxvt*color4:       #738099
URxvt*color12:      #7090cc
URxvt*color5:       #857399
URxvt*color13:      #a685cc
URxvt*color6:       #7a9998
URxvt*color14:      #8fccca
URxvt*color7:       #b3b3b3
URxvt*color15:      #cccccc

